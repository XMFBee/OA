<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
        <!-- 郭昶 struts-progress课程进度配置文件 -->
<struts>
	<package name="progress-struts" extends="my-struts" namespace="/progress">
		<action name="*" class="com.oa.action.ProgressAction" method="{1}">
			<result name="Id" type="json"></result>
			<result name="pager" type="json"></result>
			<result name="save" type="json"></result>
			<result name="update" type="json"></result>
			<result name="type" type="json"></result>
			<result name="dis" type="json"></result>
			<result name="ena" type="json"></result>
			<result name="queryByName" type="json"></result>
			<result name="showpager">/WEB-INF/pagers/educational/progress_home.jsp</result>
		</action>
	</package>
	
	
	
		
</struts>        