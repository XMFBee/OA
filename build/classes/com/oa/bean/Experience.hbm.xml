<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!-- 郭昶 -->
<hibernate-mapping package="com.oa.bean">
	<class name="Experience" table="t_exp">
		<id name="expId" column="expid">
			<generator class="uuid.hex"></generator>
		</id>
		<property name="company" column="company"></property>
		<property name="startDay" column="startday"></property>
		<property name="endDay" column="endday"></property>
		<property name="jobTitle" column="jobtitle"></property>
		<property name="des" column="des"></property>
		
	</class>
</hibernate-mapping>